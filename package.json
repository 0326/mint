{
  "name": "mint-editor",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "description": "MintEditor Project",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test --parallel",
    "start-core": "yarn workspace @mint-editor/core run start",
    "test-core": "yarn workspace @mint-editor/core run test",
    "typecheck": "turbo run typecheck",
    "version": "lerna version",
    "publish": "lerna publish from-git"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0326/mint.git"
  },
  "author": "0326",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0326/mint/issues"
  },
  "homepage": "https://github.com/0326/mint#readme",
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@types/jest": "^29.2.2",
    "esbuild": "^0.15.13",
    "eslint": "^8.27.0",
    "jest": "^29.3.1",
    "lerna": "^6.0.3",
    "turbo": "^1.6.3",
    "typescript": "^4.8.4"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
